{
  "problemMatcher": [
    {
      "owner": "flake8-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(?:\\w+: )?([^:]*):(\\d+):(\\d+): ([A-Z]\\d\\d\\d) (.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "mypy",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(?:(\\d+):)? error: (.*?)(?:  \\[(\\S+)\\])?$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    },
    {
      "owner": "pytest",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^(tests\\/.+?\\.py):(\\d+): (.+)$",
          "file": 1,
          "line": 2,
          "message": 3
        }
      ]
    }
  ]
}
